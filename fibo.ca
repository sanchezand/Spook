fun fibonacci(n:decimal)
	def a, b : decimal
	a = 0
	b = 1
	if n<0 then
		return -1
	else
		if n==0 then
			return a
		else
			if n==1 then
				return b
			else
				def i : decimal
				i = 2
				repeat i<n do
					def c : decimal
					c = a+b
					a = b
					b = c
				end
				return b
			end
		end
	end
end


