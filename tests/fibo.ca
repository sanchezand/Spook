fun fibonacci(n:decimal)
	def a, b, temp : decimal
	a = 1
	b = 0
	while n>=0 do
		temp = a
		a = a+b
		b = temp
		n = n-1
	end
	return b
end

fun fiboRecursion(n: decimal)
	if n<2 then
		return n
	else
		return fiboRecursion(n-1) + fiboRecursion(n-2)
	end
end

fun start()
	def n : decimal
	n = 5

	print(fibonacci(5))
	print(fiboRecursion(5))
end